# platform :ios, '9.0'

target 'Weibo' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Weibo
  source 'https://github.com/CocoaPods/Specs.git'
  platform :ios, '8.0'
  
  pod 'AFNetworking', '~> 3.0'
  pod 'SnapKit', '~> 4.0.0'
  pod 'SDWebImage', '~> 4.3.3'
  pod 'SVProgressHUD', '~> 2.2.5'

end
